package member.store;

import java.util.ArrayList;
import member.info.*;


public class store {

	private static store instance = new store(); 
	
	private store() {}
	
	public static store getInstance() {
		return instance; 
	}
	public info Description(int id2, String name2, String addy, double don)
	{
		info in = new info();
		in.setId(id2);
		in.setAddress(addy);
		in.setName(name2);
		in.setDonation(don);
		return in;
	}
}
